{
    "resource": {
        "dirs": {
            "test1": "test1"
        },
        "default": "test1"
    },
    "service": {
        "web_dir": "web/dist",
        "api_port": 8080,
        "port": 9876
    },
    "tracker": {
        "db_file": "ani_track.db",
        "save_interval": "1h",
        "check_interval": "1h",
        "update_interval": "1h",
        "untrack_timeout": "30d",
        "episode_watch_end_ratio": 0.9
    },
    "download": {
        "concurrent": 5,
        "retry": 5,
        "retry_interval": "1m",
        "timeout": "1h",
        "tmp_dir": "/tmp/ani_track"
    }
}